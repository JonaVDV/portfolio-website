{
  "private": true,
  "name": "jvdv-portfolio",
  "version": "1.0.0",
  "description": "The monorepo for my portfolio website",
  "scripts": {
    "prepare": "husky",
    "packages:dev": "pnpm --parallel dev",
    "format": "pnpm --parallel format",
    "format:fix": "pnpm --parallel format --write",
    "lint": "pnpm --parallel lint",
    "lint:fix": "pnpm --parallel lint:fix",
    "lint:ws": "pnpm dlx sherriff@latest",
    "ui:storybook": "pnpm -F ui storybook"
  },
  "engines": {
    "pnpm": ">=10.20.0"
  },
  "packageManager": "pnpm@10.20.0",
  "keywords": [],
  "author": "Jona van der Vlist",
  "license": "MIT",
  "devDependencies": {
    "husky": "^9.1.7"
  }
}
